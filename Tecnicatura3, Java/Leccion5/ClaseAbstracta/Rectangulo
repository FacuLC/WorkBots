package domain;

public abstract class Rectangulo extends FiguraGeometrica{      
    // Constructor
    public Rectangulo(String tipoFigura){
        super(tipoFigura);
    }
    
    @Override
    public void dibujar(){ // implementando el m√©todo
        System.out.println("Se imprime un: "+this.getClass().getSimpleName());
    }
}
